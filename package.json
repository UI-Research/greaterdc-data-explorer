    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "rewire": "^4.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
